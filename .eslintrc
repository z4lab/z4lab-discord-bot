{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "node": true
  },
  "rules": {
    "indent": [
      "error",
      "tab"
    ],
    "handle-callback-err": [
      "error"
    ]
  }
}